          <qual:input qual:id="tr_S_36_in_S_25" qual:qualitativeSpecies="S_25" qual:sign="negative" qual:thresholdLevel="1" qual:transitionEffect="none">
            <notes>
              <body xmlns="http://www.w3.org/1999/xhtml">
                <p>Decrease of delta-EF1 expression by BMP siganlling (but not shown in chondrocyte: breast cancer)</p></body>
            </notes>
          </qual:input>
          <qual:input qual:id="tr_S_36_in_S_46" qual:qualitativeSpecies="S_46" qual:sign="positive" qual:thresholdLevel="1" qual:transitionEffect="none">
